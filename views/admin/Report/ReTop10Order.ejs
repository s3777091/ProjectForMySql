<%- include('../sections/header.ejs') %>
<%- include('../sections/menu.ejs') %>
<table class="styled-table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Customer Name</th>
            <th>Order Total</th>
            <th>Order Date</th>
        </tr>
    </thead>
    <tbody>
        <% usr.forEach(function(ur){ %>
        <tr class="active-row">
            <td><%= ur.OrderID %></td>
            <td><%= ur.FullName %></td>
            <td><%= ur.Total %></td>
            <td><%= ur.Date %></td>
        </tr>
        <% }); %>
    </tbody>
</table>