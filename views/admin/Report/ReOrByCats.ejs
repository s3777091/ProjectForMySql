<%- include('../sections/header.ejs') %>
<%- include('../sections/menu.ejs') %>
<table class="styled-table">
    <thead>
        <tr>
            <th>Category ID</th>
            <th>Category Name</th>
            <th>Total Products in Category</th>
            <th>Total Order in Category</th>
            <th>Total Order in Revenue</th>
        </tr>
    </thead>
    <tbody>
        <% usr.forEach(function(ur){ %>
        <tr class="active-row">
            <td><%= ur.CategoryID %></td>
            <td><%= ur.CategoryName %></td>
            <td><%= ur.Total_orders %></td>
            <td><%= ur.Total_products %></td>
            <td><%= ur.Total_revenue %></td>
        </tr>
        <% }); %>
    </tbody>
</table>